{
    "name": "zaimea/groups-sdk-laravel",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "A Laravel service provider for including the Zaimea Groups SDK for PHP",
    "keywords": [
        "zaimea.com",
        "groups",
        "sdk"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": ">=8.1",
        "zaimea/zaimea-groups-sdk-php": "^1.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zaimea\\GroupsSDK\\": "/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Zaimea\\GroupsSDK\\GroupsServiceProvider"
            ],
            "aliases": {
                "GROUPS": "Zaimea\\GroupsSDK\\GroupsFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
